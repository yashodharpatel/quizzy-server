{
  "name": "quizzy-server",
  "version": "1.0.0",
  "description": "A backend for a quiz application built with Node.js, Express, MongoDB, and TypeScript.",
  "main": "dist/app.js",
  "type": "module",
  "scripts": {
    "start": "node --experimental-specifier-resolution=node dist/app.js",
    "dev": "nodemon --exec node --loader ts-node/esm src/app.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "test": "jest"
  },
  "author": "Yashodhar Patel",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yashodharpatel/quizzy-server.git"
  },
  "bugs": {
    "url": "https://github.com/yashodharpatel/quizzy-server/issues"
  },
  "homepage": "https://github.com/yashodharpatel/quizzy-server#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongoose": "^7.7.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.8.2",
    "@typescript-eslint/eslint-plugin": "^6.7.1",
    "@typescript-eslint/parser": "^6.7.1",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
