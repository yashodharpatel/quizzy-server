{
  "name": "quizzy-server",
  "version": "1.0.0",
  "description": "A backend for a quiz application built with Node.js, Express, MongoDB, and TypeScript.",
  "main": "dist/app.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon --exec node --loader ts-node/esm src/app.ts",
    "build": "npx rollup -c",
    "start": "npm run build && node dist/app.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "test": "jest"
  },
  "author": "Yashodhar Patel",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yashodharpatel/quizzy-server.git"
  },
  "bugs": {
    "url": "https://github.com/yashodharpatel/quizzy-server/issues"
  },
  "homepage": "https://github.com/yashodharpatel/quizzy-server#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongoose": "^7.7.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.8.2",
    "@typescript-eslint/eslint-plugin": "^6.7.1",
    "@typescript-eslint/parser": "^6.7.1",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "rollup": "^4.18.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
